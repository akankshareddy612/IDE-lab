using System;
using System.Collections.Generic;
using System.Linq;
namespace myApp
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine("Hello World!");
        }
    }
}



using System;
using System.Collections.Generic;
using System.Linq;
namespace myApp
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine("Hello Karthik");
        }
    }
}



using System;
using System.Collections.Generic;
using System.Linq;
namespace myApp
{
    class Program
    {
        static void Main()
        {
            var name = "Karthik";
            Console.WriteLine("Hello " + name + "!");
        }
    }
}



using System;
using System.Collections.Generic;
using System.Linq;
namespace myApp
{
    class Program
    {
        static void Main()
        {
            var name = "Karthik";
            Console.WriteLine($"Hello {name}!");
        }
    }
}



using System;
using System.Collections.Generic;
using System.Linq;
namespace myApp
{
    class Program
    {
        static void Main()
        {
            var name = "Karthik";
            Console.WriteLine($"Hello {name.ToUpper()}!");
        }
    }
}



using System;
using System.Collections.Generic;
using System.Linq;
namespace myApp
{
    class Program
    {
        static void Main()
        {
            var names = new List<string> { "Karthik", "Satya Sai", "Chilukuri" };
            foreach (var name in names)
            {
                Console.WriteLine($"Hello {name.ToUpper()}!");
            }
        }
    }
}
