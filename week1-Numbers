        }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
decimal c = 1.0M;
decimal d = 3.0M;
Console.WriteLine(c / d);
        }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
double radius = 2.50;
double area = Math.PI * radius * radius;
Console.WriteLine(area);
        }
    }
}
