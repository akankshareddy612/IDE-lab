using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int a = 10;
int b = 3;
int c = a + b;
int d = a - b;
int e = a/b;
int f = a*b;
Console.WriteLine(c);
Console.WriteLine(d);
Console.WriteLine(e);
Console.WriteLine(f);   
        }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int a = 18;
int b = 6;
int c = a + b;
int d = a - b;
int e = a/b;
int f = a*b;
Console.WriteLine(c);
Console.WriteLine(d);
Console.WriteLine(e);
Console.WriteLine(f);
        }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int a = 5;
int b = 4;
int c = 2;
int d = (a + b) * c;
Console.WriteLine(d);
        }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int a = 5;
int b = 4;
int c = 2;
int d = (a + b) - 6 * c + (12 * 4) / 3 + 12;
Console.WriteLine(d);
         }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int a = 7;
int b = 4;
int c = 3;
int d = (a + b) / c;
Console.WriteLine(d);
        }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int a = 7;
int b = 4;
int c = 3;
int d = (a + b) / c;
int e = (a + b) % c;
Console.WriteLine($"quotient: {d}");
Console.WriteLine($"remainder: {e}");
         }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int max = int.MaxValue;
int min = int.MinValue;
Console.WriteLine($"The range of integers is {min} to {max}");
        }
    }
}



using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
int max = int.MaxValue;
int min = int.MinValue;
Console.WriteLine($"The range of integers is {min} to {max}");
int what = max + 3;
Console.WriteLine($"An example of overflow: {what}");
        }
    }
}



